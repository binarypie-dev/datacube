[Unit]
Description=Datacube - Data provider service for application launchers
Documentation=https://github.com/binarypie-dev/datacube
After=graphical-session.target

[Service]
Type=simple
ExecStart=/usr/bin/datacube
Restart=on-failure
RestartSec=5
Environment=RUST_LOG=info

[Install]
WantedBy=default.target
